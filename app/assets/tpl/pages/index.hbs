{{#extend 'base'}}
	{{#content 'wr'}}
		<div class="offer-money_back">
			<div class="container-fluid">
				<div class="row">
					<div class="col-md-12">

						{{>elements/socials it_education.body.html.author.socials}}
						
						{{>offer offer.money_back}}
						
						{{#carousel testimonials.base i="4" class="js-carousel"}}
							{{>testimonial face_top="true" class="default"}}
						{{/carousel}}

						<div class="decor">
							<img src="#" alt="">
							<img src="#" alt="">
							<img src="#" alt="">
						</div>

						{{>modules/content it_education.body.html class="author"}}

						{{!-- <ul>
							{{#clone html.faq i="4" start="4"}}
							<li>
								{{>faq-item}}
							</li>
						</ul> --}}
						
						{{>modules/title sale.it class="title--couching"}}
						{{>modules/form class="start"}}
					</div>
				</div>
			</div>
		</div>
		<div class="testimonials-default">
			<div class="container-fluid">
				<div class="row">
					<div class="col-md-12">
						
						{{>modules/title sale.it single="h3"}}
						{{>testimonial}}

						<ul>
							<li><i class="seoicon-lightbulb-on"></i> <span>482</span> открытых вакансий <a href="#">прямо сейчас</a></li>
						</ul>

					</div>
				</div>
			</div>
		</div>
		<div class="testimonials-promo">
			<div class="container-fluid">
				<div class="row">
					<div class="col-md-12">
						
					</div>
				</div>
			</div>
		</div>
		<div class="sale-you">
			<div class="container">
				<div class="row">
					<div class="col-md-12">
						
						{{>modules/title sale.you}}

						{{>modules/content sale.you class="author"}}

					</div>
				</div>
				<div class="row">
					<div class="col-lg-6 col-md-12 col-sm-12 col-xs-12">
						
						<p>{{sale.you.subtitle}}</p>
						<ul class="listing">
							{{#each sale.you.reasons}}
							<li><i class="seoicon-check"></i> {{this}}</li>
							{{#if @last}}<li><i class="fa fa-heart"></i> {{this}}</li>{{/if}}
							{{/each}}
						</ul>

					</div>
					<div class="col-lg-6 col-md-12 col-sm-12 col-xs-12">
						
						<figure><img src="{{sale.you.img}}" alt=""></figure>

					</div>
				</div>
			</div>
		</div>
		<div class="sale-need">
			<div class="container-fluid">
				<div class="row">
					<div class="col-lg-6 col-md-6 col-sm-12 col-xs-12 no-padding">
						
						{{!-- {{> modules/video}} --}}

					</div>
					<div class="col-lg-4 col-lg-offset-1 col-md-4 col-md-offset-1 col-sm-12 col-xs-12 no-padding">
						
						{{>modules/content sale.need}}

					</div>
				</div>
			</div>
		</div>
		<div class="sale-it">
			<div class="container">
				<div class="row">
					<div class="col-lg-8 col-lg-offset-2 col-md-8 col-md-offset-2 col-sm-12 col-xs-12">
						
						
						{{>modules/title sale.it}}
						
 
					</div>
				</div>
				<div class="row">
					<div class="col-lg-3 col-md-3 col-sm-12 col-xs-12">
						
					</div>
					<div class="col-lg-3 col-md-3 col-sm-12 col-xs-12">
						
					</div>
					<div class="col-lg-3 col-md-3 col-sm-12 col-xs-12">
						
					</div>
					<div class="col-lg-3 col-md-3 col-sm-12 col-xs-12">
						
					</div>
				</div>
				<div class="row">
					<div class="col-md-12">

					</div>
				</div>
			</div>
		</div>
		<div class="testimonials-promo">
			<div class="container">
				<div class="row">
					<div class="col-lg-4 col-md-4 col-sm-12 col-xs-12">
						
						

					</div>
					<div class="col-lg-7 col-lg-offset-1 col-md-8 col-sm-12 col-xs-12">
						
						

					</div>
				</div>
			</div>
		</div>
		<div class="promo-our_courses">
			<div class="container">
				<div class="row">
					{{!-- {{#each it_education.preview}}
					<div class="col-lg-4 col-md-6 col-sm-6 col-xs-12">
						
						{{>course_preview}}

					</div>
					{{/each}} --}}
				</div>
			</div>
		</div>
	{{/content}}
{{/extend}}